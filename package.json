{
  "name": "tiptap-clean-paste",
  "version": "0.0.1",
  "description": "extenion for the Tiptap editor that provides clean paste functionality, allowing users to strip unwanted content like formatting and styles from pasted text ",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicscode/tiptap-clean-paste"
  },
  "keywords": [
    "tiptap",
    "editor",
    "clean",
    "paste",
    "plugin",
    "extension"
  ],
  "author": "Koceila LOUNAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unicscode/tiptap-clean-paste/issues"
  },
  "homepage": "https://github.com/unicscode/tiptap-clean-paste",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "dev": "npm run clean && rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@tiptap/core": "^2.0.0-beta.220",
    "@tiptap/pm": "^2.0.0-beta.220",
    "rollup": "^3.17.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@tiptap/core": "^2.0.0-beta.220",
    "@tiptap/pm": "^2.0.0-beta.220"
  }
}
